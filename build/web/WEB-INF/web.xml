<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>inicioSesion</servlet-name>
        <servlet-class>servlet.inicioSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registrarFuncionario</servlet-name>
        <servlet-class>servlet.registrarFuncionario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReporteFinca</servlet-name>
        <servlet-class>servlet.ReporteFinca</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>mostrar</servlet-name>
        <servlet-class>servlet.mostrar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registrarAnimal</servlet-name>
        <servlet-class>servlet.registrarAnimal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistroVeterinario</servlet-name>
        <servlet-class>servlet.RegistroVeterinario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProduccionLeche</servlet-name>
        <servlet-class>servlet.ProduccionLeche</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistroInseminacion</servlet-name>
        <servlet-class>servlet.RegistroInseminacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistroHistoria</servlet-name>
        <servlet-class>servlet.RegistroHistoria</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>inicioSesion</servlet-name>
        <url-pattern>/iniciar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registrarFuncionario</servlet-name>
        <url-pattern>/registrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReporteFinca</servlet-name>
        <url-pattern>/ConsultaFinca</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mostrar</servlet-name>
        <url-pattern>/mostrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registrarAnimal</servlet-name>
        <url-pattern>/creaAnimal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistroVeterinario</servlet-name>
        <url-pattern>/regVeterinario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProduccionLeche</servlet-name>
        <url-pattern>/ReporteLeche</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistroInseminacion</servlet-name>
        <url-pattern>/creaInseminacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistroHistoria</servlet-name>
        <url-pattern>/creaHistoria</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
